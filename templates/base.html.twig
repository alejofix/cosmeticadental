<!-- login modal -->

{{ include('cleaners/loginmodal.html.twig') }}

<div id="box_wrapper">

    <section id="topline" class="light_section table_section">
        {{ include('cleaners/topline.html.twig') }}
    </section>

    <header id="header">
        {{ include('cleaners/menu.html.twig') }}
    </header>

    <section id="breadcrumbs" class="light_section with_bottom_border">
        {#
        {{ include('cleaners/section.html.twig') }}
        #}
    </section>

    {% block body %}

    {% endblock %}

    <footer id="footer" class="grey_section">
        {{ include('cleaners/footer.html.twig') }}
